// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sushi {
  id          Int    @id @default(autoincrement())
  photo       String
  name        String
  description String
  price       Float
}

model Menu {
  id    Int    @id @default(autoincrement())
  photo String
}
